{
    "namespace": "https://car-spa.io/",
    "events": [

        /*
        * "maintenance" event should be triggered from the client 
        * to the server then the server will trigger the "maintenance" 
        * in the client to receive notification
        */

        {
            "event": "maintenance",
            "type": "on",
            "data": "none",
            "owner": "server"
        },
        {
            "event": "maintenance",
            "type": "emit",
            "data": { "message": "string" },
            "owner": "client"
        },

        /*
        * "offer" event should be triggered from the client 
        * to the server then the server will trigger the "offer" 
        * in the client to receive notification
        */

        {
            "event": "offer",
            "type": "on",
            "data": "none",
            "owner": "server"
        },
        {
           "event": "offer",
           "type": "emit",
           "data": { "message": "string" },
           "owner": "client"  
        }
    ]
}